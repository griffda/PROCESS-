shmatm(3,0:maxmat) dble  # IFE shield material masses (kg) (region,material)
shmatv(3,0:maxmat) dble  # IFE shield material volumes (m3) (region,material)
shvol      dble          # IFE shield volume (m3)
sombdr     dble /2.7/    # radius of cyl blkt section below chamber (ifetyp=2)
somtdr     dble /2.7/    # radius of cyl blkt section above chamber (ifetyp=2)
tdspmw     dble /0.01/FIX# IFE target delivery system power (MW)
tfacmw     dble          # IFE target factory power (MW)
tgain      dble /85.0/   # IFE target gain (if ifedrv = 0)              var.83
uccarb     dble /50.0/   # cost of carbon cloth ($/kg)
ucconc     dble /0.1/    # cost of concrete ($/kg)
ucflib     dble /84.0/   # cost of FLiBe ($/kg)
uctarg     dble /0.3/    # cost of IFE target ($/target)
v1dr       dble /0.0/    # radial thickness of IFE void between FW and BL (m)
v1dzl      dble /0.0/    # vertical thickness of IFE void 1 below chamber (m)
v1dzu      dble /0.0/    # vertical thickness of IFE void 1 above chamber (m)
v1matf(3,0:maxmat) dble /./# IFE void 1 material fractions (region,material) 
